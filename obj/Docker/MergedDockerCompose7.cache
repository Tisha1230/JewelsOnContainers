services:
  catalog:
    build:
      context: D:\Tisha Bootcamp\JewelsOnContainers\ProductCatalogAPI
      dockerfile: D:\Tisha Bootcamp\JewelsOnContainers\ProductCatalogAPI\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ProductCatalogAPI
      target: base
    container_name: jewelscatalogapi
    depends_on:
      mssqlserver:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      DatabaseName: CatalogDb
      DatabasePassword: ProductApi(!)
      DatabaseServer: mssqlserver
      DatabaseUser: sa
      ExternalCatalogUrl: http://localhost:7000
      NUGET_FALLBACK_PACKAGES: ""
    image: jewels/catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/ProductCatalogAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$(pidof
        dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      backend: null
      frontend: null
    ports:
    - mode: ingress
      target: 80
      published: 7000
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\Tisha Bootcamp\JewelsOnContainers\ProductCatalogAPI
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\Tisha Bootcamp\JewelsOnContainers\ProductCatalogAPI
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\prera\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\prera\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\prera\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\prera\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  mssqlserver:
    container_name: jewlssqlserver
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: ProductApi(!)
    image: mcr.microsoft.com/mssql/server:latest
    networks:
      backend: null
    ports:
    - mode: ingress
      target: 1433
      published: 1448
      protocol: tcp
  webmvc:
    build:
      context: D:\Tisha Bootcamp\JewelsOnContainers\WebMvc
      dockerfile: D:\Tisha Bootcamp\JewelsOnContainers\WebMvc\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebMvc
      target: base
    container_name: webshop
    depends_on:
      catalog:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      CatalogUrl: http://catalog
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: jewels/webmvc:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/WebMvc.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$(pidof
        dotnet); then kill $PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      frontend: null
    ports:
    - mode: ingress
      target: 80
      published: 7500
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\Tisha Bootcamp\JewelsOnContainers\WebMvc
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\Tisha Bootcamp\JewelsOnContainers\WebMvc
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\prera\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\prera\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\prera\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
