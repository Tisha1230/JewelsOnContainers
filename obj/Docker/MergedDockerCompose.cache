networks:
  backend: {}
  frontend: {}
services:
  catalog:
    build:
      context: C:\Users\tisha\Desktop\Bootcamp\Microservices\JewelsOnContainers\ProductCatalogAPI
      dockerfile: Dockerfile
    container_name: jewelscatalogapi
    depends_on:
      mssqlserver:
        condition: service_started
    environment:
      DatabaseName: CatalogDb
      DatabasePassword: ProductApi(!)
      DatabaseServer: mssqlserver
      DatabaseUser: sa
    image: jewels/catalogapi
    networks:
      backend: null
      frontend: null
    ports:
    - published: 7000
      target: 80
  mssqlserver:
    container_name: jewlssqlserver
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: ProductApi(!)
    image: mcr.microsoft.com/mssql/server:latest
    networks:
      backend: null
    ports:
    - published: 1448
      target: 1433
version: '3.4'